{
  "name": "chat-6-demo",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config ./proxy.json",
    "start-secure": "ng serve -c secure --proxy-config ./proxy-secure.json --ssl --ssl-key key.pem --ssl-cert cert.pem",
    "pre-load-app-data": "ts-node ./server/database/pre-load-app-data.ts",
    "start-server": "echo '*** Be sure you have run build with ctrl-shift-b ***' && node server/dist/out-tsc/server.js",
    "build-server": "./node_modules/.bin/tsc -p ./server/tsconfig.json",
    "build": "ng build --prod --aot && npm run build-server",
    "test": "ng test --watch",
    "api-test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run build",
    "dep-pre-load-app-data-local": "ts-node ./server/database/pre-load-app-data.ts --dbHost localhost --dbPort 6379 --dbAuth this_should_be_a_secret_authcode",
    "dep-start-server-local": "echo '*** Be sure you have run build with ctrl-shift-b ***' && node server/dist/out-tsc/server.js --dbHost localhost --dbPort 6379 --dbAuth this_should_be_a_secret_authcode",
    "dep-server": "./node_modules/.bin/nodemon  -w ./server --ext \".ts\" --exec \"npm run start-server\"",
    "dep-start-server-prod": "echo '*** Be sure you have built front end app and run server build with ctrl-shift-b ***' && node dist/server.js --secure --prod",
    "dep-start-image-server": "./node_modules/.bin/ts-node ./server/image-server.ts",
    "dep-start-redisdb": "redis-server ./server/database/redis.conf",
    "dep-pre-load-user-data": "ts-node ./server/database/pre-load-user-data.ts",
    "dep-pre-load-app": "npm run pre-load-user-data && npm run pre-load-app-data",
    "dep-redis-cli": "redis-cli -p 6379"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.1",
    "@angular/cdk": "^6.0.1",
    "@angular/cli": "6.0.1",
    "@angular/common": "6.0.1",
    "@angular/compiler": "6.0.1",
    "@angular/compiler-cli": "6.0.1",
    "@angular/core": "6.0.1",
    "@angular/forms": "6.0.1",
    "@angular/http": "6.0.1",
    "@angular/material": "^6.0.1",
    "@angular/platform-browser": "6.0.1",
    "@angular/platform-browser-dynamic": "6.0.1",
    "@angular/router": "6.0.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "argon2": "^0.16.1",
    "argon2-ffi": "^1.1.0",
    "autoprefixer": "^7.2.6",
    "bldg25-chat": "^1.3.5",
    "bldg25-chat-server": "^1.3.5",
    "body-parser": "^1.17.2",
    "bootstrap": "^4.1.1",
    "command-line-args": "^4.0.6",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express": "^4.16.3",
    "express-http-proxy": "^1.1.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "http-server": "^0.10.0",
    "jsonwebtoken": "^7.4.2",
    "moment": "^2.18.1",
    "ng2-validation": "^4.2.0",
    "password-validator": "^4.0.0",
    "redis": "^2.8.0",
    "rxjs": "^6.2.1",
    "typescript": "2.7.2",
    "ws": "^5.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular/language-service": "6.0.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/form-data": "^2.2.1",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.2",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/node": "^10.5.2",
    "@types/redis": "^2.8.5",
    "@types/ws": "^5.1.2",
    "codelyzer": "^4.1.0",
    "concurrently": "^3.5.1",
    "frisby": "^2.0.16",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jest": "^23.3.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.15.1",
    "protractor": "~5.1.2",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "tslint": "~5.7.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/"
    ]
  },
  "engines": {
    "node": "10.4.0",
    "npm": "6.2.0"
  }
}
